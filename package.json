{
  "name": "nrafunded",
  "version": "2.0.2",
  "description": "Let's take Congress back from the NRA.",
  "private": true,
  "scripts": {
    "dev": "webpack-dev-server",
    "build": "NODE_ENV=production webpack",
    "start": "npm run dev",
    "scrape": "node parse/scraper.js",
    "parse": "node parse/parse.js",
    "data": "npm run scrape && npm run parse",
    "render": "npm run render:analysis && npm run render:body",
    "render:analysis": "static-react --no-doctype src/Analysis.js -o docs/analysis.html --id analysis",
    "render:body": "static-react --no-doctype src/Body.js -o docs/body.html --i body",
    "flag": "repng src/Flag.js -w 512 -h 512 -o docs -f flag"
  },
  "devDependencies": {
    "axios": "^0.21.1",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "comma-number": "^2.0.0",
    "css-loader": "^0.28.7",
    "html-loader": "^0.5.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-geomicons": "^2.1.0",
    "react-static": "^1.2.1",
    "rebass": "^1.0.4",
    "scrape-it": "^3.3.3",
    "static-react": "^4.0.0-2",
    "style-loader": "^0.19.0",
    "uglify-es-webpack-plugin": "^0.10.0",
    "uglifyjs-webpack-plugin": "^1.0.0-beta.3",
    "unformat-usd": "^0.1.1",
    "velocity-react": "^1.3.3",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  },
  "author": "Lachlan Campbell <lachlan.campbell@icloud.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lachlanjc/nrafunded"
  }
}
